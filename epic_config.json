{
  "folder_templates": [
    {
      "root_folder": "~/Downloads",
      "folders": [
        "Compressed",
        "Documents",
        "Pictures",
        "Music",
        "Video",
        "Programs",
        "Unsorted",
        "Misc",
        "Misc/Unsorted",
        "Misc/No extensions",
        "Misc/Folders"
      ],
      "place_for_unwanted": "~/Downloads/Folders"
    }
  ],
  "operations": [
    {
      "scan_sources": [
        "~/Downloads",
        "~/Pictures",
        "~/Downloads/Unsorted"
      ],
      "rules": [
        {
          "keywords": null,
          "extensions": [
            "zip",
            "7z",
            "tar",
            "bz2",
            "rar",
            "xz",
            "gz"
          ],
          "whitelist": null,
          "destination": "~/Downloads/Compressed"
        },
        {
          "keywords": null,
          "extensions": [
            "jar"
          ],
          "whitelist": null,
          "destination": "~/Downloads/Compressed/Java"
        },
        {
          "keywords": null,
          "extensions": [
            "exe",
            "elf",
            "bin",
            "deb",
            "rpm",
            "msi",
            "appimage"
          ],
          "whitelist": null,
          "destination": "~/Downloads/Programs"
        },
        {
          "keywords": null,
          "extensions": [
            "mp3",
            "mp2",
            "wav",
            "ogg",
            "aac",
            "flac",
            "alac",
            "dsd",
            "mqa",
            "m4a"
          ],
          "whitelist": null,
          "destination": "~/Downloads/Music"
        },
        {
          "keywords": null,
          "extensions": [
            "mid"
          ],
          "whitelist": null,
          "destination": "~/Downloads/Music/midi"
        },
        {
          "keywords": [
            "wallpaper",
            "unsplash"
          ],
          "extensions": [
            "jpeg",
            "jpg",
            "png"
          ],
          "whitelist": null,
          "destination": "~/Pictures/wallpaper"
        },
        {
          "keywords": null,
          "extensions": [
            "jpg",
            "png"
          ],
          "whitelist": null,
          "destination": "~/Pictures/"
        },
        {
          "keywords": [
            "screenshot"
          ],
          "extensions": null,
          "whitelist": null,
          "destination": "~/Pictures/Screenshot"
        },
        {
          "keywords": null,
          "extensions": [
            ""
          ],
          "whitelist": null,
          "destination": "~/Downloads/Misc/No extensions"
        },
        {
          "keywords": null,
          "extensions": [
            "mp4",
            "mkv"
          ],
          "whitelist": null,
          "destination": "~/Downloads/Video"
        }
      ]
    },
    {
      "scan_sources": [
        "~/Downloads"
      ],
      "rules": [
        {
          "keywords": "",
          "extensions": null,
          "whitelist": null,
          "destination": "~/Downloads/Unsorted/"
        }
      ]
    }
  ]
}